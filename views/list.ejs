<%- include('header'); -%>

  <body>
    <div class="box" id="heading">
      <h1>
        <%= listTitle %>
      </h1>
    </div>

    <div class="box">

      <% newlistItems.forEach((items)=>{ %>

        <div class="item">
          <input type="checkbox" />
          <p>
            <%= items.name %>
          </p>
        </div>

        <% }) %>

          <form class="item" action="/" method="post">
            <input type="text" name="newItem" placeholder="Add New" autocomplete="off" />
            <button type="submit" name="list" value=<%=listTitle %>>+</button>
          </form>
    </div>
    <%- include('footer'); -%>
  </body>

  </html>
